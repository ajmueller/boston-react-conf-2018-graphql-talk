{
  "name": "graphql-user-search-sample",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Chris Toomey <chris@ctoomey.com>",
  "license": "MIT",
  "scripts": {
    "check:types": "tsc --noEmit --skipLibCheck",
    "check:lint": "tslint --project .",
    "start": "react-scripts-parcel start",
    "gql:types":
      "apollo codegen:generate --queries=src/**/*.tsx --schema src/gqlSchema.json --target typescript --outputFlat src/gqlTypes.ts"
  },
  "devDependencies": {
    "apollo": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "create-react-app-parcel-typescript": "^0.0.5",
    "prettier": "^1.11.1",
    "tsc": "^1.20150623.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "apollo-client": "^2.2.7",
    "dotenv-expand": "^4.2.0",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "react": "^16.2.0",
    "react-apollo": "^2.1.0",
    "react-dom": "^16.2.0",
    "react-octicons": "^0.2.0",
    "react-router-dom": "^4.2.2",
    "styled-components": "^3.2.5"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [">1%", "last 4 versions", "Firefox ESR", "not ie < 11"]
  }
}
